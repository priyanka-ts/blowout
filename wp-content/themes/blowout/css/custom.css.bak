  height: 100%;
  font-family: 'Lato', sans-serif;
}
body {
  /* Required margin for .navbar-fixed-top. Remove if using .navbar-static-top. Change if height of navigation changes. */
    background-color:transparent;
}
html.full {
    background: url(../images/bg-img-1.png) no-repeat center center fixed;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    background-size: cover;
    -o-background-size: cover;
}
.navbar-inverse {
	background-color: transparent;
	border: none;
  min-height: 0px;
}

.navbar-inverse .navbar-nav > li > a {
  color: #282828;
  font-size: 14px;
  -webkit-transition: all ease .2s;
       -moz-transition: all ease .2s;
         -o-transition: all ease .2s;
            transition: all ease .2s;
  font-weight: 300;
  font-family: 'Lato', sans-serif;
  letter-spacing: 1px;
}
.navbar-inverse .navbar-nav > li > a:hover,
.navbar-inverse .navbar-nav > li > a:focus {
  color: #444444;
  background-color: rgba(255,255,255, 0.6);
  -webkit-transition: all ease .2s;
       -moz-transition: all ease .2s;
         -o-transition: all ease .2s;
            transition: all ease .2s;
}
.navbar-inverse .navbar-nav > .active > a,
.navbar-inverse .navbar-nav > .active > a:hover,
.navbar-inverse .navbar-nav > .active > a:focus {
  color: #444;
  background-color: rgba(255,255,255, 0.8);
}

.navbar-brand,
.navbar-nav li a {
    line-height: 60px;
    height: 60px;
    padding-top: 0;
}

.navbar-brand {
  padding-top: 20px;
  padding-left: 40px;
}

.top-nav-brand {
  background-color: #fbf8f7;
  border-bottom: 5px solid #A3725A;
  padding: 20px;
}

.book-nav-button {
